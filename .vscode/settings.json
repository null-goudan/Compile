{
    "C_Cpp.errorSquiggles": "Enabled",
    "python.linting.flake8Enabled": true,
    "python.formatting.provider": "yapf",
    "python.linting.flake8Args": ["--max-line-length=248"],
    "python.linting.pylintEnabled": false,
    "files.associations": {
        "iostream": "cpp",
        "*.cjson": "jsonc",
        "*.wxss": "css",
        "*.wxs": "javascript",
        "iomanip": "cpp",
        "ostream": "cpp",
        "cstdlib": "cpp",
        "bitset": "cpp",
        "chrono": "cpp",
        "algorithm": "cpp",
        "*.tcc": "cpp",
        "limits": "cpp",
        "type_traits": "cpp",
        "iosfwd": "cpp",
        "typeindex": "cpp",
        "typeinfo": "cpp",
        "random": "cpp",
        "sstream": "cpp"
    }
}